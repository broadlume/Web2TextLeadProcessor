{
  "name": "web2text-service",
  "version": "1.0.0",
  "description": "Web2Text Lead Processor",
  "main": "app.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc --noEmitOnError",
    "prebundle": "rm -rf dist",
    "bundle": "esbuild src/app.ts --bundle --minify --sourcemap --platform=node --target=es2020 --outfile=dist/app.js",
    "app": "NODE_ENV=production node ./dist/app.js",
    "app-dev": "tsx watch ./src/app.ts --inspect=9229",
    "reset": "bun run clear-restate; bun run clear-dynamodb || true;",
    "register-with-restate": "bun run clear-restate; restate deployments reg web2text-devcontainer:9080 --force --yes --quiet",
    "clear-restate": "/app/scripts/clearRestate.sh Lead /app/scripts/clearRestate.sh Dealer",
    "clear-restate-test": "/app/scripts/clearRestate.sh Lead-test && /app/scripts/clearRestate.sh Dealer-test",
    "clear-dynamodb": "LOCAL_DYNAMODB_URL=http://web2text-dynamodb-local:8000 /app/scripts/clearDynamoDb.sh DEV",
    "clear-dynamodb-test": "LOCAL_DYNAMODB_URL=http://web2text-dynamodb-local:8000 /app/scripts/clearDynamoDb.sh DEV",
    "e2e": "cd e2e_tests && bun run vitest --config './vitest.config.ts'"
  },
  "dependencies": {
    "@elastic/ecs-winston-format": "^1.5.3",
    "@restatedev/restate": "^1.0.2",
    "@restatedev/restate-sdk": "1.3.1",
    "@twilio/conversations": "^2.5.0",
    "dotenv": "^16.4.5",
    "dynamoose": "^4.0.1",
    "ky": "^1.7.2",
    "libphonenumber-js": "^1.11.4",
    "tsafe": "^1.7.2",
    "twilio": "^5.2.0",
    "winston": "^3.14.2",
    "zod": "^3.23.8",
    "zod-validation-error": "^3.3.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.2",
    "@types/node": "^20.12.7",
    "@types/shelljs": "^0.8.15",
    "@types/supertest": "^6.0.2",
    "esbuild": "^0.18.12",
    "nock": "^14.0.0-beta.11",
    "shelljs": "0.8.5",
    "supertest": "^7.0.0",
    "tsx": "4.16.2",
    "typescript": "^5.4.5",
    "vitest": "^2.1.1"
  },
  "patchedDependencies": {
    "shelljs@0.8.5": "patches/shelljs@0.8.5.patch",
    "@restatedev/restate-sdk@1.3.1": "patches/@restatedev%2Frestate-sdk@1.3.1.patch"
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "core-js",
    "esbuild"
  ]
}
